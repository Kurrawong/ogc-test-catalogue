PREFIX addr:    <https://linked.data.gov.au/def/addr/>
PREFIX geo:     <http://www.opengis.net/ont/geosparql#>
PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema:  <https://schema.org/>
PREFIX dbo:     <http://dbpedia.org/ontology/>

<http://example.org/australia_boundingbox>
        a geo:Geometry;
        geo:asWKT "POLYGON((112.76 -10.23, 155.48 -10.23, 155.48 -44.28, 112.76 -44.28, 112.76 -10.23))"^^geo:wktLiteral .

<http://example.org/ogc-test-dataset>
    a schema:Dataset ;
    schema:name "OGC Test Dataset" ;
    schema:description "Some sample Geo Features"@en ;
    rdfs:member <http://example.org/ogc-test-features> .

<http://example.org/ogc-test-features>
    a geo:FeatureCollection ;
    schema:name "OGC Test Features" ;
    geo:hasBoundingBox <http://example.org/australia_boundingbox> ;
    rdfs:member <http://example.org/australia> ,
                <https://linked.data.gov.au/dataset/qld-addr/address/2fd46078-88c0-5f30-b43e-d2908d9445b6> ,
                <https://linked.data.gov.au/dataset/qld-addr/address/65cb1e52-fc1d-5dee-a2d2-ea7882d12c7e> ,
                <https://linked.data.gov.au/dataset/qld-addr/address/7036d80a-ecb6-5ec9-86f0-5e5135934e04> .

<http://example.org/australia>
        schema:isPartOf <http://example.org/ogc-test-features> ;
        a                geo:Feature, dbo:Country;
        rdfs:label       "A broad bounding box of Australia for testing purposes"@en;
        geo:hasGeometry <http://example.org/australia_boundingbox> .

